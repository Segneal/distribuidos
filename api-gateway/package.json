{
  "name": "ong-api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for Sistema ONG Backend",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:postman": "node postman/run-tests.js",
    "test:postman:auth": "node postman/run-tests.js --collection=01-Autenticacion",
    "test:postman:users": "node postman/run-tests.js --collection=02-Usuarios",
    "test:postman:inventory": "node postman/run-tests.js --collection=03-Inventario",
    "test:postman:events": "node postman/run-tests.js --collection=04-Eventos",
    "test:postman:network": "node postman/run-tests.js --collection=05-Red-ONGs",
    "migrate": "node src/scripts/migrate.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.10",
    "@grpc/reflection": "^1.0.3",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "grpc-health-check": "^2.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mysql2": "^3.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "grpc-tools": "^1.12.4",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.3"
  },
  "keywords": [
    "ong",
    "api-gateway",
    "microservices"
  ],
  "author": "Sistema ONG Team",
  "license": "MIT"
}
